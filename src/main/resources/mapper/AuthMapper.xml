<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ssafy.dongsanbu.domain.auth.mapper.AuthMapper">

    <select id="findByUsernameAndPassword" parameterType="com.ssafy.dongsanbu.domain.auth.dto.LoginDto"
            resultType="user">
        SELECT *
        FROM member
        WHERE username = #{username}
          and password = #{password}
    </select>

</mapper>